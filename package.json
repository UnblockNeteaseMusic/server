		"@swc/core": "^1.2.158",
		"@types/node": "^17.0.21",
		"@types/pino": "6.3.12",
		"browserslist": "^4.20.2",
		"core-js": "^3.21.1",
		"jest": "^27.5.1",
		"pkg": "^5.5.2",
